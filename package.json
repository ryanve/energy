{
  "name": "energy",
  "description": "Simple cross-platform event emitter",
  "version": "0.8.0",
  "homepage": "https://github.com/ryanve/energy",
  "license": "CC0-1.0",
  "author": "Ryan Van Etten",
  "main": "energy.js",
  "ender": "noop",
  "repository": {
    "type": "git",
    "url": "https://github.com/ryanve/energy.git"
  },
  "scripts": {
    "pretest": "jshint energy.js && jshint test.js",
    "test": "node test"
  },
  "devDependencies": {
    "aok": "^1.9.0",
    "deep-equal": "^1.0.1",
    "jshint": "^2.12.0"
  },
  "keywords": [
    "event-emitter",
    "eventemitter",
    "events",
    "event",
    "emitter",
    "emit",
    "ender",
    "listener",
    "listeners",
    "on",
    "pubsub",
    "javascript",
    "observer",
    "trigger"
  ],
  "jshintConfig": {
    "asi": true,
    "boss": true,
    "browser": true,
    "debug": true,
    "devel": true,
    "eqnull": true,
    "evil": true,
    "expr": true,
    "globals": {
      "define": true,
      "ender": true
    },
    "jquery": true,
    "lastsemic": true,
    "latedef": "nofunc",
    "laxcomma": true,
    "maxcomplexity": 15,
    "maxerr": 10,
    "maxlen": 115,
    "maxparams": 4,
    "node": true,
    "sub": true,
    "supernew": true,
    "trailing": true,
    "undef": true,
    "unused": true,
    "worker": true
  }
}
